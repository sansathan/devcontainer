{
    "name": "Node Dev Container",
    "image": "ghcr.io/sansathan/devcontainer-base:latest",
    "features": {
        "ghcr.io/devcontainers/features/node": {
            "version": "lts"
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "esbenp.prettier-vscode",
                "dbaeumer.vscode-eslint",
                "apollographql.vscode-apollo",
                "mquandalle.graphql",
                "fill-labs.dependi"
            ]
        }
    }
}