    "image": "ghcr.io/sansathan/devcontainer-base:latest",
    "features": {
        "ghcr.io/devcontainer-community/devcontainer-features/bun.sh": {
            "version": "latest"
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "oven.bun-vscode",
                "apollographql.vscode-apollo",
                "mquandalle.graphql",
                "fill-labs.dependi",
                "biomejs.biome"
            ]
        }
    }
}